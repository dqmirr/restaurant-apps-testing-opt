{
    "env": {
        "browser": true,
        "es2021": true,
        "node" : true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-underscore-dangle": 0,
        "import/no-unresolved": [
            0, 
            { "caseSensitive": false }
         ],
         "import/extensions":0,
         "import/no-extraneous-dependencies": 0,
         "no-extra-semi": 0
    }
}
